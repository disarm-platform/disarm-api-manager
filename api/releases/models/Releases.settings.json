{
  "connection": "default",
  "collectionName": "releases",
  "info": {
    "name": "releases",
    "description": "Release of an algo, function, model, etc..."
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "version": {
      "default": "",
      "type": "string"
    },
    "container_link": {
      "default": "",
      "type": "string"
    },
    "docs_link": {
      "default": "",
      "type": "string"
    },
    "published": {
      "default": false,
      "type": "boolean"
    },
    "algo": {
      "model": "algo",
      "via": "releases"
    },
    "runs": {
      "collection": "run",
      "via": "releases"
    }
  }
}